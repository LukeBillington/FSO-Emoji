{
  "manifest_version": 2,

  "name": "FSO Emoji",
  "description": "This replaces your grades with emojis that represent them.",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],

  "icons": {
    "16": "fse-18.png",
    "48": "fse-48.png",
    "128": "fse-128.png"
  },

  "content_scripts": [{
    "run_at": "document_end",
    "js": ["content.js"],
    "matches": ["https://online.fullsail.edu/*"]
  }]

}
